{
  "name": "blue-hunter",
  "version": "1.0.0",
  "description": "Teste: https://github.com/mpguedes/blue-hunter",
  "main": "index.js",
  "scripts": {
    "test": "karma start",
    "start": "webpack-dev-server --inline --progress --port 8080 --config webpack.config.dev.js",
    "build:prod": "del-cli dist && ngc -p tsconfig.aot.json && ngc -p tsconfig.aot.json && webpack --config webpack.config.prod.js --progress --profile --bail && del-cli src/app/**/*.js src/app/**/*.ngfactory.ts src/app/**/*.js.map src/app/**/*.shim.ts src/app/**/*.ngsummary.json src/app/**/*.ngstyle.ts dist/app dist/src dist/app dist/src dist/node_modules",
    "clean": "del-cli dist src/app/**/*.js src/app/**/*.ngfactory.ts src/app/**/*.js.map src/app/**/*.shim.ts src/app/**/*.ngsummary.json src/app/**/*.ngstyle.ts",
    "build": "webpack --config webpack.config.dev.js --progress",
    "serve": "lite-server",
    "patch": "bump --patch --commit --tag",
    "minor": "bump --minor --commit --tag",
    "major": "bump --major --commit --tag",
    "pre-release": "bump --prerelease --commit --tag"
  },
  "author": "Jerson Janke",
  "license": "ISC",
